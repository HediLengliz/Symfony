<h1>List of  authors</h1>
<table border="1">
    <tr>
        <th>id</th>
        <th>Username</th>
        <th>Email</th>
        <th>Nb_books</th>
    </tr>

    {% for i in tabAuthors %}
        <tr>
            <td> {{ i.id }}</td>
            <td>{{ i.username }}</td>
            <td>{{ i.email }}</td>
            <td>{{ i.nb_books }}</td>
        </tr>
    {% endfor %}
</table>
